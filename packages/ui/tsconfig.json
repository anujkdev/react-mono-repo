{
  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}